<div class="beerpartial-container">
<div class="beerpartial-img">
   <a href="/beers/{{this.id}}"><img src="{{this.image_url}}" alt="beer"></a>
</div>
<div id="beers-partial-info">
    <h1>{{this.name}}</h1>
    <p>{{this.description}}</p>
    <h4>{{this.tagline}}</h4>
    {{#if randomBeer}}
        <ul>
            {{#each this.food_pairing}}
                <li>{{this}}</li>
            {{/each}}
        </ul>
    {{/if}}
</div>
</div>